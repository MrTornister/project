{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "init-db": "NODE_OPTIONS=\"--loader ts-node/esm\" node src/scripts/initDatabase.ts",
    "dev": "vite",
    "server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc && vite build",
    "start": "npm run init-db && npm run dev:all",
    "build:node": "tsc --project tsconfig.node.json",
    "generate-orders": "npm run build:node && node dist/node/src/scripts/generateOrders.js"
  },
  "dependencies": {
    "@types/react-beautiful-dnd": "^13.1.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^11.1.0",
    "googleapis": "^105.0.0",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.1.1",
    "react-tooltip": "^5.28.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.5",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-modal": "^3.16.3",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
